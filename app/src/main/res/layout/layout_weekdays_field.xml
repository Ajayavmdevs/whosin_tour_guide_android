<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:layout_marginVertical="@dimen/_3sdp"
    android:orientation="horizontal">


    <TextView
        android:id="@+id/fromTitle"
        style="@style/txt11Regular"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_marginStart="@dimen/_3sdp"
        android:gravity="center"
        android:paddingVertical="@dimen/_5sdp"
        android:paddingEnd="@dimen/_10sdp"
        android:text="From" />

    <com.whosin.app.comman.ui.roundcornerlayout.RoundCornerConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="#3A3A3A"
        app:corner_radius="@dimen/_5sdp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <TextView
            android:id="@+id/tvFromDate"
            style="@style/txt10Regular"
            android:layout_width="@dimen/_85sdp"
            android:layout_height="match_parent"
            android:gravity="center"
            android:paddingHorizontal="@dimen/_14sdp"
             android:paddingVertical="@dimen/_5sdp"
            android:text="12/9/2024" />


    </com.whosin.app.comman.ui.roundcornerlayout.RoundCornerConstraintLayout>

    <TextView
        android:id="@+id/tillTitle"
        style="@style/txt11Regular"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:gravity="center"
        android:paddingHorizontal="@dimen/_8sdp"
        android:paddingVertical="@dimen/_5sdp"
        android:text="Till" />

    <com.whosin.app.comman.ui.roundcornerlayout.RoundCornerConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="#3A3A3A"
        app:corner_radius="@dimen/_5sdp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <TextView
            android:id="@+id/tvTillDate"
            style="@style/txt10Regular"
            android:layout_width="@dimen/_85sdp"
            android:layout_height="match_parent"
            android:gravity="center"
            android:paddingHorizontal="@dimen/_14sdp"
            android:paddingVertical="@dimen/_5sdp"
            android:text="12/9/2024" />


    </com.whosin.app.comman.ui.roundcornerlayout.RoundCornerConstraintLayout>


</LinearLayout>