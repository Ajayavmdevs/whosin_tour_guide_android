                <androidx.constraintlayout.widget.ConstraintLayout
                    android:id="@+id/cmEditProfile"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginHorizontal="@dimen/_15sdp"
                    android:layout_marginTop="@dimen/_15sdp"
                    android:background="@drawable/edit_profile_bg"
                    app:layout_constraintTop_toBottomOf="@+id/roundConstraint">

                    <TextView
                        style="@style/txt10Regular"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:gravity="center"
                        android:padding="@dimen/_6sdp"
                        android:text="Edit Profile"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toTopOf="parent" />

                </androidx.constraintlayout.widget.ConstraintLayout>


                <androidx.recyclerview.widget.RecyclerView
                    android:id="@+id/eventListNameRecyclerView"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/_10sdp"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/cmEditProfile" />

                <androidx.constraintlayout.widget.ConstraintLayout
                    android:id="@+id/profileScoreView"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/_10sdp"
                    android:visibility="gone"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/eventListNameRecyclerView">

                    <TextView
                        android:id="@+id/tvName"
                        style="@style/txt14Bold"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginHorizontal="@dimen/_15sdp"
                        android:text="Profile Score"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toTopOf="parent" />

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/_10sdp"
                        android:gravity="center_horizontal"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toBottomOf="@id/tvName">

                        <com.whosin.app.ui.controller.ComplementryProfileViews.ComplementryProfileScoreView
                            android:id="@+id/profilePunctuality"
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_marginHorizontal="@dimen/_10sdp"
                            android:layout_weight="1"
                            app:layout_constraintEnd_toStartOf="@id/profileValue"
                            app:layout_constraintStart_toStartOf="parent"
                            app:layout_constraintTop_toTopOf="parent"
                            app:scoreTitleText="Punctuality" />

                        <com.whosin.app.ui.controller.ComplementryProfileViews.ComplementryProfileScoreView
                            android:id="@+id/profileActivity"
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_marginEnd="@dimen/_10sdp"
                            android:layout_weight="1"
                            app:layout_constraintEnd_toStartOf="@id/profileValue"
                            app:layout_constraintStart_toEndOf="@id/profilePunctuality"
                            app:layout_constraintTop_toTopOf="parent"
                            app:scoreTitleText="Activity" />

                        <com.whosin.app.ui.controller.ComplementryProfileViews.ComplementryProfileScoreView
                            android:id="@+id/profileValue"
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_marginEnd="@dimen/_10sdp"
                            android:layout_weight="1"
                            app:layout_constraintEnd_toEndOf="parent"
                            app:layout_constraintStart_toEndOf="@id/profileActivity"
                            app:layout_constraintTop_toTopOf="parent"
                            app:scoreTitleText="Value" />
                    </LinearLayout>

                </androidx.constraintlayout.widget.ConstraintLayout>

                <androidx.constraintlayout.widget.ConstraintLayout
                    android:id="@+id/ratingTitleLayout"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_20sdp"
                    android:layout_marginTop="@dimen/_15sdp"
                    android:layout_marginEnd="@dimen/_15sdp"
                    android:visibility="gone"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/profileScoreView">

                    <TextView
                        android:id="@+id/tvRate"
                        style="@style/txt14Bold"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:text="Rating &amp; Reviews"
                        app:layout_constraintBottom_toBottomOf="parent"
                        app:layout_constraintEnd_toStartOf="@id/rating"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toTopOf="parent" />

                    <me.zhanghai.android.materialratingbar.MaterialRatingBar
                        android:id="@+id/rating"
                        style="@style/Widget.MaterialRatingBar.RatingBar"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_gravity="end"
                        android:layout_marginStart="@dimen/_5sdp"
                        android:isIndicator="true"
                        android:stepSize="1.0"
                        app:layout_constraintBottom_toBottomOf="parent"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintStart_toEndOf="@id/tvRate"
                        app:layout_constraintTop_toTopOf="parent"
                        app:mrb_progressBackgroundTint="@color/white"
                        app:mrb_progressTint="@color/brand_pink"
                        app:mrb_secondaryProgressTint="@color/brand_pink"
                        tools:ignore="SpeakableTextPresentCheck" />

                </androidx.constraintlayout.widget.ConstraintLayout>

                <androidx.recyclerview.widget.RecyclerView
                    android:id="@+id/ratingReviewRecycler"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:nestedScrollingEnabled="false"
                    android:visibility="gone"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/ratingTitleLayout"
                    tools:itemCount="1"
                    tools:listitem="@layout/item_all_rating_list" />

                <androidx.constraintlayout.widget.ConstraintLayout
