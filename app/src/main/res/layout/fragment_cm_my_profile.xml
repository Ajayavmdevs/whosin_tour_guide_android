                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent">

                    <org.imaginativeworld.whynotimagecarousel.ImageCarousel
                        android:id="@+id/imageCarousel"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:visibility="visible"
                        app:autoPlay="true"
                        app:autoPlayDelay="4000"
                        app:imageScaleType="centerCrop"
                        app:indicatorColor="@color/white_40"
                        app:indicatorMargin="0dp"
                        app:layout_constraintBottom_toBottomOf="parent"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toTopOf="parent"
                        app:showCaption="false"
                        app:showIndicator="false"
                        app:showNavigationButtons="false" />

                    <ImageView
                        android:id="@+id/cmImageCover"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:scaleType="centerCrop"
                        android:src="@drawable/background_cover_image"
                        android:visibility="gone" />

                    <com.whosin.app.comman.ui.roundcornerlayout.RoundCornerConstraintLayout
                        android:id="@+id/profileswitchBtn"
